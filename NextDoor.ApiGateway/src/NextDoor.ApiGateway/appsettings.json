{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "mvc": {
    "name": "NextDoor Api Gateway"
  },
  "host": {
    "port": 5200
  },
  "serilog": {
    "consoleEnabled": true,
    "level": "information"
  },
  "rabbitMq": {
    "namespace": "apigateway",
    "retries": 3,
    "retryInterval": 2,
    "username": "guest",
    "password": "guest",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      "localhost"
    ],
    "requestTimeout": "00:00:10",
    "publishConfirmTimeout": "00:00:01",
    "recoveryInterval": "00:00:10",
    "persistentDeliveryMode": true,
    "autoCloseConnection": true,
    "automaticRecovery": true,
    "topologyRecovery": true,
    "exchange": {
      "durable": true,
      "autoDelete": false,
      "type": "Topic"
    },
    "queue": {
      "autoDelete": false,
      "durable": true,
      "exclusive": false
    }
  },
  "restEase": {
    "loadBalancer": "",
    "services": [
      {
        "name": "identity-service",
        "host": "localhost",
        "scheme": "http",
        "port": "5201"
      },
      {
        "name": "customer-service",
        "host": "localhost",
        "scheme": "http",
        "port": "5202"
      },
      {
        "name": "notification-service",
        "host": "localhost",
        "scheme": "http",
        "port": "5209"
      },
      {
        "name": "admin-service",
        "host": "localhost",
        "scheme": "http",
        "port": "5203"
      }
    ]
  }
}
